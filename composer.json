{
    "name": "rabsana/coinbase",
    "description": "Coinbase API library",
    "keywords": [ "bitcoin", "coinbase" ],
    "homepage": "https://github.com/rabsana/coinbase",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [{
        "name": "Sajjad Sisakhti",
        "email": "sajjad.30sakhti@gmail.com",
        "homepage": "https://rabsana.ir"
    }],
    "autoload": {
        "psr-4": { "Coinbase\\Wallet\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Coinbase\\Wallet\\Tests\\": "tests/" }
    },
    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/guzzle": "^7.2",
        "psr/http-message": "^1.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
